// Set canvas size
size(600, 600);

// Set border width for all shapes
strokeWeight(4);

// Set canvas color
background(10, 110, 110);

// Draw blue circle
// with orange border
fill(2, 255, 255);
stroke(255, 191, 0);
ellipse(110, 300, 200, 200);

// Draw yellow square
// with pink border
fill(242, 255, 0);
stroke(255, 0, 191);
rect(180, 390, 200, 200);

// Draw red triangle
// with green border
fill(244, 77, 65);
stroke(113, 244, 66);
triangle(30, 75, 450, 20, 590, 550);
